# pratical scripting language for syncro client

HOST 127.0.0.1 PORT 31337 DELAY 10 SYS socketcall RT 3 -- /bin/ping 127.0.0.1 -c 2 
HOST 127.0.0.1 PORT 31338 DELAY 10 SYS write           -- /bin/ps aux 
